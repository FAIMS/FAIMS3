  "description": "",
  "devDependencies": {
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.9.0",
    "@types/node": "^22.2.0",
    "eslint-plugin-prettier": "^5.2.1",
    "gts": "^5.3.1",
    "turbo": "^2.0.11",
    "typescript": "^5.9.2"
  },
  "packageManager": "pnpm@10.7.0",
  "dependencies": {
    "@capacitor-community/exif": "^7.0.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
