    "ol": "^6.8.1",
    "pouchdb": "^7.2.2",
    "pouchdb-adapter-memory": "^7.2.2",
    "pouchdb-find": "^7.2.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-share": "^4.4.0",
    "uuid": "^8.1.0",
    "web-vitals": "^1.1.1",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@types/node": "^14.11.2",
    "@types/yup": "^0.29.11",
    "fast-check": "^2.14.0",
    "gts": "^3.1.0",
    "jest": "^26.6.0",
    "jest-fast-check": "^1.0.2",
    "react-scripts": "4.0.3",
    "typescript": "^4.4.3"
