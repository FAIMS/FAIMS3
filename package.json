{
  "name": "@faims-project/faims3",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "20.2.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "scripts": {
    "start": "GENERATE_SOURCEMAP=false && craco start",
    "build": "craco --max-old-space-size=4096 build",
    "pretest": "npm run compile",
    "test": "NODE_ENV=test craco test --setupFilesAfterEnv=./setupTests.js --env=./test/custom-test-env.js --forceExit --testPathIgnorePatterns=integration_tests --transformIgnorePatterns \"node_modules/(?!ol)/\"",
    "posttest": "npm run lint",
    "preintegration": "npm run compile",
    "integration": "NODE_ENV=test react-scripts test --env=node --forceExit --runInBand --detectOpenHandles src/integration_tests --transformIgnorePatterns \"node_modules/(?!ol)/\"",
    "postintegration": "npm run lint",
    "eject": "react-scripts eject",
    "preserve": "npm run build",
    "serve": "npx serve --config ../serve.json --single --no-clipboard build",
    "prewebapp-build": "npm run build",
    "webapp-build": "cap copy",
    "webapp-sync": "cap sync",
    "app-update": "cap update",
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "doc": "jsdoc -c ./jsdoc.config.json"
  },
