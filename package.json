{
  "name": "faims3",
  "version": "1.2.8",
  "main": "index.js",
  "engines": {
    "node": "22"
  },
  "scripts": {
    "build-app": "pnpm run build --workspace=app",
    "build-api": "pnpm run build --workspace=api",
    "build-data-model": "pnpm run build --workspace=library/data-model",
    "github-build-app": "pnpm run build --workspace=library/data-model --workspace=app",
    "docker-build-api": "pnpm run build --workspace=library/data-model --workspace=api",
    "build-aws": "pnpm run build --workspace=infrastructure/aws-cdk",
    "package-aws": "pnpm run package --workspace=infrastructure/aws-cdk",
    "test-app": "pnpm test --workspace=app",
    "serve-app": "pnpm run serve --workspace=app",
    "start": "pnpm start --workspace=api --workspace=app",
    "start-api": "pnpm start --workspace=api",
    "start-app": "pnpm start --workspace=app",
    "start-web": "pnpm start --workspace=web",
    "force-start-app": "pnpm force-start --workspace=app",
    "watch-api": "pnpm run local --workspace=api",
    "web-dev": "pnpm run dev --workspace=web",
    "start-designer": "pnpm run start --workspace=designer",
    "migrate": "pnpm run migrate --workspace=api",
    "migrate-with-keys": "pnpm run migrate-with-keys --workspace=api",
    "load-projects": "pnpm run load-projects --workspace=api",
    "load-templates": "pnpm run load-templates --workspace=api",
    "generate-local-keys": "./api/keymanagement/makeInstanceKeys.sh",
    "build-web": "turbo build --filter web",
    "dev": "turbo dev --filter=@faims3/app --filter=web --filter=@faims3/api --filter=@faims3/data-model --parallel",
    "lint": "turbo lint",
    "clean": "turbo clean",
    "build": "turbo build"
  },
