{
  "name": "@faims3/app",
  "packageManager": "pnpm@10.7.0",
  "version": "1.3.1",
  "private": true,
  "type": "module",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": "22"
  },
  "scripts": {
    "prestart": "pnpm run runconfig",
    "start": "vite",
    "force-start": "vite --force",
    "dev": "pnpm run start",
    "prebuild": "pnpm run runconfig",
    "build": "tsc && vite build",
    "pretest": "pnpm run compile",
    "build-sourcemap": "pnpm run build -- --mode sourcemap",
    "bugsnag-upload": "env-cmd ./bin/bugsnag-sourcemaps.sh",
    "test": "vitest --watch=false",
    "test-watch": "vitest",
    "posttest": "pnpm run lint",
    "precoverage": "pnpm run compile",
    "coverage": "vitest run --coverage --watch=false",
    "preintegration": "pnpm run compile",
    "integration": "NODE_ENV=test react-scripts test --env=node --forceExit --runInBand --detectOpenHandles src/integration_tests --transformIgnorePatterns \"node_modules/(?!ol)/\"",
    "postintegration": "pnpm run lint",
    "preserve": "pnpm run build",
    "serve": "vite preview",
    "prewebapp-build": "pnpm run build",
    "webapp-build": "cap copy",
    "webapp-sync": "cap sync",
    "app-update": "cap update",
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "doc": "jsdoc -c ./jsdoc.config.json",
    "runconfig": "env-cmd --no-override --silent --use-shell ./bin/prebuildConfig.sh",
    "configIOSbuild": "env-cmd --no-override --silent ./bin/prebuildIOS.sh"
  },
  "dependencies": {
    "@bugsnag/js": "^7.20.2",
    "@bugsnag/plugin-react": "^7.19.0",
    "@bugsnag/source-maps": "^2.3.3",
    "@capacitor-community/exif": "^7.0.0",
    "@capacitor-mlkit/barcode-scanning": "^7.2.1",
    "@capacitor/android": "^7.2.0",
    "@capacitor/app": "^7.0.1",
    "@capacitor/app-launcher": "^7.0.1",
    "@capacitor/browser": "^7.0.1",
    "@capacitor/camera": "^7.0.1",
    "@capacitor/core": "^7.2.0",
    "@capacitor/device": "^7.0.1",
    "@capacitor/filesystem": "^7.0.1",
    "@capacitor/geolocation": "^7.1.1",
    "@capacitor/ios": "^7.2.0",
    "@capacitor/share": "^7.0.1",
    "@capacitor/status-bar": "^7.0.1",
    "@emotion/react": "11.11.4",
    "@emotion/styled": "11.10.8",
    "@faims3/data-model": "workspace:*",
    "@faims3/forms": "workspace:*",
    "@fast-check/vitest": "^0.0.7",
    "@ionic/pwa-elements": "^3.2.2",
    "@mui/icons-material": "5.15.8",
    "@mui/lab": "5.0.0-alpha.129",
    "@mui/material": "5.15.14",
    "@mui/x-data-grid": "6.20.4",
    "@reduxjs/toolkit": "^2.5.0",
    "@tanstack/react-query": "^5.62.16",
    "@types/cordova": "11.0.0",
    "@types/jest": "29.5.1",
    "@types/lodash": "^4.14.194",
    "@types/markdown-it": "^14.1.1",
    "@types/node": "^22.13.14",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "@types/react-router": "5.1.20",
    "@types/react-router-dom": "5.3.3",
    "@types/react-router-hash-link": "^2.4.9",
    "@types/trusted-types": "^2.0.3",
    "@types/uuid": "9.0.1",
    "animate.css": "^4.1.1",
    "assert": "^2.0.0",
    "buffer": "^6.0.3",
    "clsx": "1.2.1",
    "jose": "5.2.3",
    "jsonpointer": "5.0.1",
    "levelup": "^5.1.1",
    "lodash": "4.17.21",
    "markdown-it": "^14.1.0",
    "moment": "2.29.4",
    "ol": "^10.3.1",
    "ol-mapbox-style": "^12.5.0",
    "pouchdb-adapter-memory": "^9.0.0",
    "pouchdb-browser": "^9.0.0",
    "pouchdb-debug": "^7.2.1",
    "pouchdb-find": "^9.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-jss": "^10.10.0",
    "react-obfuscate": "^3.6.9",
    "react-redux": "^9.2.0",
    "react-router": "^6.26.0",
    "react-router-dom": "^6.26.0",
    "react-router-hash-link": "^2.4.3",
    "redux-persist": "^6.0.0",
    "redux-persist-indexeddb-storage": "^1.0.4",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "stream": "^0.0.2",
    "uuid": "9.0.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@capacitor/assets": "^3.0.5",
    "@capacitor/cli": "^7.1.0",
    "@swc/core": "^1.7.10",
    "@testing-library/jest-dom": "6.4.6",
    "@testing-library/react": "^15.0.7",
    "@testing-library/user-event": "^14.6.1",
    "@types/pouchdb": "^6.4.2",
    "@types/pouchdb-adapter-memory": "^6.1.6",
    "@types/pouchdb-browser": "^6.1.5",
    "@vitejs/plugin-react-swc": "^4.0.1",
    "@vitest/coverage-v8": "^2.1.8",
    "esbuild": "^0.25.0",
    "eslint-plugin-react": "^7.32.2",
    "fast-check": "2.25.0",
    "gts": "^5.3.1",
    "jsdoc": "^4.0.2",
    "jsdom": "^22.1.0",
    "jss": "^10.10.0",
    "msw": "^2.3.1",
    "node-gyp": "^9.3.1",
    "type-fest": "^4.20.0",
    "typescript": "^5.9.2",
    "vite": "^7.1.11",
    "vite-plugin-svgr": "^4.3.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^2.1.8"
  }
}
